{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "standard",
    "plugin:import/typescript",
    "plugin:import/warnings",
    "plugin:import/errors"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": [
          "src"
        ]
      }
    }
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {}
}